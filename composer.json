{
    "name": "drewwynne0/curl-easy",
    "description": "cURL wrapper for PHP. Supports parallel and non-blocking requests. For high speed crawling, see stil/curl-robot.",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/drewwynne0/curl-easy"
        }
    ],
    "replace": {
        "symfony/event-dispatcher": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "cURL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cURL\\Tests\\": "tests/"
        }
    }
}
